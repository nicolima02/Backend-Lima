<div class="chat-cont">
    <form class="formulario_chat" action="/">
        <input type="email" placeholder="usuario" class="inputUser">
    
    <div id="mensajes">
            <p>bienvenido al chat</p>
            {{#each chatCompleto}}
            <div class="estructura">
                <p id="user">{{mensaje.user}}</p>
                <p id="date">[{{mensaje.date}}]</p>
                <p id="message">:{{mensaje.msg}}</p>`
            </div>
            {{/each}}
    </div>
    <div>
        <input type="text" class="enviarmsg" placeholder="Escriba aqui..." >
        <a href="#bottom"><button class="boton_enviar">Enviar</button></a>
    </div>
    </form>
</div>

<div id="bottom"></div>

<script src="https://cdnjs.cloudflare.com/ajax/libs/qs/6.10.1/qs.min.js"
    integrity="sha512-aTKlYRb1QfU1jlF3k+aS4AqTpnTXci4R79mkdie/bp6Xm51O5O3ESAYhvg6zoicj/PD6VYY0XrYwsWLcvGiKZQ=="
    crossorigin="anonymous" referrerpolicy="no-referrer"></script>
